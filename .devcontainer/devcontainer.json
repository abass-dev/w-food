{
    "name": "Wajabatt Food Dev Container",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "app",
    "workspaceFolder": "/app",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/sh"
    },
    "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "prisma.prisma",
        "bradlc.vscode-tailwindcss"
    ],
    "forwardPorts": [
        3000
    ],
    "postCreateCommand": "npm install",
    "remoteUser": "node"
}